export main;

type Main = Act { Time(Int) };

var::Int = 0;

act x::Main {
  -> for n::Int in 0..1000 do var := var + 1;
  Time(i::Int) -> var := var + 1
};

act main::Main {  

  -> for n::Int in 0..100 do {
       new x
     };
  Time(n::Int) when n > 1000 -> { print[Int](var); stopAll() };
  Time(n::Int) -> { }

}

















