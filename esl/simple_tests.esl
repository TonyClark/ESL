f(x) = x + 10;

g(a,b) = a + b;

act a {
  Time(t) -> print(t);
  Up(20) -> {
    print(Upper())
  };
  Down(0) -> {
    print(Low())
  };
  Up(x) -> {
    print(Up(x))
  };
  Down(x) -> {
    print(Down(x))
  }
};

y = g(f(g(f(100),f(100))),f(100)); 

addTree(t) = 
  case t {
    Tree(left,right) ->
      (addTree(left)) + addTree(right);
    Leaf(n) -> n
  };
  
map(f,l) =
  case l {
    [a,'x',true] -> [3];
    [a,'x',false] -> [4];
    [a,'x',_] -> [print(5),print(6)];
    h:t -> (f(h)):map(f,t);
    [] -> []
  };

test1() = addTree(Tree(Leaf(y),Tree(Leaf(200),Leaf(300))));

